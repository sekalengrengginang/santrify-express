// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id   Int  @id @default(autoincrement())
  name String @unique
  email String @unique
  password String 
}

model Santri {
  id Int @id @default(autoincrement())
  name String
  room String
  class String 
  status String @db.Text
}

model Room{
  id Int @id @default(autoincrement())
  name String
  status String
}

model Class{
  id Int @id
  
}
